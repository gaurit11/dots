!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Fonts -- !
!*.font:xft:mononoki Nerd Font:pixelsize=15
URxvt.font: xft:mononoki Nerd Font:size=12,xft:Inconsolata Nerd Font Mono:size=11, xft:FontAwesome:pixelsize=13, xft:Unifont:size=10 
URxvt.boldfont:xft:Unifont:size=10,xft:Iosevka:pixelsize=15,xft:MaterialIcons:size=10
! window padding

URxvt.letterSpace: 0
URxvt.internalBorder:  10
URxvt.scrollBar: false

